<!DOCTYPE html>
<html>
<head>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-eOJMYsd53ii+scO/bJGFsiCZc+5NDVN2yr8+0RDqr0Ql0h+rP48ckxlpbzKgwra6" crossorigin="anonymous">
 <link rel="stylesheet" href="/assets/main2.css">

<link href="https://fonts.googleapis.com/css?family=Open+Sans" rel="stylesheet">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>Leads</title>
</head>
<body>

<div class="container">
  <h1>Plasma donors Details</h1>
<div class="container">


<div class="row">

<% arr.forEach(function(arr1){ %>

<div class="col-lg-4 col-md-6 col-sm-12">
	<div class="card">
    <div class="card-body">
      <h3 class="card-title"><%= arr1.donorname %></h3>
      <!-- <span></span> -->
      <p class="card-text"><%= arr1.donorage %></p>
      <p class="card-text"><%= arr1.donorcontact %></p>
      <p class="card-text"><%= arr1.address %></p>
      <p class="card-text"><%= arr1.city %></p>
      <p class="card-text">Recovered on: <%= arr1.negativedate %></p>
    </div>

  </div>
</div>
<% });%>
</div>
</div>
<script
  src="https://code.jquery.com/jquery-3.6.0.js"
  integrity="sha256-H+K7U5CnXl1h5ywQfKtSj8PCmoN9aaq30gDh27Xc0jk="
  crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta3/dist/js/bootstrap.bundle.min.js" integrity="sha384-JEW9xMcG8R+pH31jmWH6WWP0WintQrMb4s7ZOdauHnUtxwoG2vI5DkLtS3qm9Ekf" crossorigin="anonymous"></script>

</body>

</html>